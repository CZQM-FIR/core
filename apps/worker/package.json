{
  "name": "worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=es2020 --format=esm --outfile=dist/index.js --external:arktype --external:dotenv --external:node-cron --external:@libsql/client --external:express --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\"",
    "build:tsc": "tsc",
    "start": "tsx src/index.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250321.0",
    "@eslint/js": "^9.18.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.20.0",
    "wrangler": "^4.5.0"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.787.0",
    "@czqm/db": "workspace:*",
    "@czqm/common": "workspace:*",
    "@libsql/client": "^0.15.1",
    "arktype": "^2.1.20",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.3.1",
    "node-cron": "^4.2.1"
  }
}
